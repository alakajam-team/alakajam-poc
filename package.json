{
  "name": "alakajam",
  "version": "2.0.0-dev",
  "description": "Website powering the Alakajam! game making community",
  "license": "MIT",
  "main": "dist/server/index.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc -w",
    "start": "node dist/server/index.js",
    "start:watch": "ts-node-dev server/index.ts patch-ormconfig-ts-node",
    "migration:run": "tsc && typeorm migration:run --config config",
    "migration:revert": "tsc && typeorm migration:revert --config config",
    "debug": "node --inspect --require ts-node/register server/index.ts",
    "lint": "tslint --project tsconfig.json --fix",
    "docs": "typedoc --out dist/docs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alakajam-team/alakajam.git"
  },
  "bugs": {
    "url": "https://github.com/alakajam-team/alakajam/issues"
  },
  "dependencies": {
    "express": "^4.16.4",
    "express-nunjucks": "^2.2.3",
    "express-promise-router": "^3.0.2",
    "find-up": "^3.0.0",
    "mkdirp": "^0.5.1",
    "moment": "^2.24.0",
    "nunjucks": "^3.2.0",
    "optional-require": "^1.0.0",
    "pg": "^7.8.2",
    "pkg-up": "^2.0.0",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^4.0.6",
    "typeorm": "^0.2.14",
    "webpack": "^4.29.6",
    "winston": "^2.4.4"
  },
  "optionalDependencies": {},
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.4",
    "@fortawesome/fontawesome-free": "^5.7.2",
    "@types/express": "^4.16.1",
    "@types/express-promise-router": "^2.0.1",
    "@types/find-up": "^2.1.1",
    "@types/node": "^10.12.30",
    "@types/nunjucks": "^3.1.1",
    "@types/winston": "^2.3.9",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.5",
    "bootstrap": "^4.3.1",
    "bootstrap.native": "^2.0.25",
    "css-loader": "^1.0.1",
    "cssnano": "^4.1.10",
    "file-loader": "^2.0.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^5.3.0",
    "style-loader": "^0.23.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.13.1",
    "typescript": "^3.3.3333",
    "url-loader": "^1.1.2",
    "vue": "^2.6.8",
    "vue-json-tree-view": "^2.1.4",
    "webpack-cli": "^3.2.3",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2"
  },
  "browserslist": [
    ">1%",
    "last 2 versions",
    "IE >= 9"
  ]
}
